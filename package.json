{
    "name": "@amazon-web-services-cloudformation/cloudformation-cli-typescript-lib",
    "version": "1.0.5",
    "description": "The CloudFormation Resource Provider Development Kit (RPDK) allows you to author your own resource providers that can be used by CloudFormation. This plugin library helps to provide runtime bindings for the execution of your providers by CloudFormation.",
    "main": "dist/index.js",
    "directories": {
        "test": "tests"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc",
        "prepack": "npm run build",
        "lint": "eslint --ext .ts,.js .",
        "lint:fix": "eslint --fix --ext .ts,.js .",
        "test": "jest",
        "test:debug": "--node-arg=--inspect jest --runInBand"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aws-cloudformation/cloudformation-cli-typescript-plugin.git"
    },
    "author": "Amazon Web Services",
    "license": "Apache License 2.0",
    "bugs": {
        "url": "https://github.com/aws-cloudformation/cloudformation-cli-typescript-plugin/issues"
    },
    "homepage": "https://github.com/aws-cloudformation/cloudformation-cli-typescript-plugin#readme",
    "dependencies": {
        "autobind-decorator": "^2.4.0",
        "class-transformer": "^0.5.1",
        "reflect-metadata": "^0.1.13",
        "string.prototype.replaceall": "^1.0.3",
        "uuid": "^7.0.2"
    },
    "devDependencies": {
        "@aws-sdk/client-cloudformation": "^3.428.0",
        "@aws-sdk/client-cloudwatch": "^3.428.0",
        "@aws-sdk/client-cloudwatch-logs": "^3.428.0",
        "@aws-sdk/client-s3": "^3.428.0",
        "@aws-sdk/client-sts": "^3.428.0",
        "@smithy/smithy-client": "^2.1.9",
        "@smithy/types": "^2.3.5",
        "@tsconfig/node14": "^1.0.1",
        "@tsconfig/node18": "^18.2.2",
        "@types/jest": "^26.0.15",
        "@types/node": "^18.0.0",
        "@types/uuid": "^7.0.0",
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "@typescript-eslint/parser": "^6.7.5",
        "@yarnpkg/sdks": "^3.0.0-rc.53",
        "eslint": "^7.13.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-import-resolver-node": "^0.3.3",
        "eslint-import-resolver-typescript": "^2.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prefer-arrow": "^1.2.2",
        "eslint-plugin-prettier": "^3.1.3",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "jest-html-reporters": "^3.1.4",
        "prettier": "^2.2.0",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.1.1",
        "typescript": "^4.9.5"
    },
    "peerDependencies": {
        "@aws-sdk/client-cloudwatch": "^3.428.0",
        "@aws-sdk/client-cloudwatch-logs": "^3.428.0",
        "@aws-sdk/client-s3": "^3.428.0"
    },
    "peerDependenciesMeta": {
        "@aws-sdk/client-cloudwatch": {
            "optional": true
        },
        "@aws-sdk/client-cloudwatch-logs": {
            "optional": true
        },
        "@aws-sdk/client-s3": {
            "optional": true
        }
    },
    "prettier": {
        "parser": "typescript",
        "singleQuote": true,
        "tabWidth": 4,
        "printWidth": 88,
        "trailingComma": "es5",
        "endOfLine": "lf"
    },
    "packageManager": "yarn@3.6.4"
}
